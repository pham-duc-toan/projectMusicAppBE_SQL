    "passport-local": "^1.0.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "7.8.1",
    "streamifier": "^0.1.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17",
    "unidecode": "^1.1.0",
    "uuid": "^11.1.0"
